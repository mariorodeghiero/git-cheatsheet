{
  "git": [
    {
      "title": "Configurations",
      "url":
        "https://git-scm.com/book/en/v2/Getting-Started-First-Time-Git-Setup",
      "command": [
        {
          "id": 1,
          "command": "git config --global user.name \"your name\"",
          "note": "Configure the author name."
        },
        {
          "id": 2,
          "command": "git config --global user.email \"your@email.com\"",
          "note": "Configure the author email."
        },
        {
          "id": 3,
          "command": "git config --global color.ui true",
          "note": "Git automatically colors most of its output."
        },
        {
          "id": 4,
          "command": "git config --global format.pretty oneline",
          "note": "Configure default format"
        },
        {
          "id": 5,
          "command": "git config --list",
          "note": "View your Git configurations."
        },
        {
          "id": 6,
          "command": "git help config",
          "note": "Getting help in configuring."
        },
        {
          "id": 7,
          "command": "git help",
          "note": "Getting Help."
        },
        {
          "id": 8,
          "command": "git init",
          "note": "Initializing Git repository."
        },
        {
          "id": 9,
          "command": "git status",
          "note":
            "List the files you've changed and those you still need to add or commit."
        }
      ]
    },
    {
      "title": "Commit History",
      "url":
        "https://git-scm.com/book/en/v2/Git-Basics-Viewing-the-Commit-History",
      "command": [
        {
          "id": 1,
          "command": "git diff --stat",
          "note":
            "Show changes between commits, commit and working tree, etc..."
        },
        {
          "id": 2,
          "command": "git log",
          "note":
            "Lists the commits made in the repository in chronological order."
        },
        {
          "id": 3,
          "command": "git log --author=name_author",
          "note": "List author"
        },
        {
          "id": 4,
          "command": "git log --pretty=oneline",
          "note": "Shows each commit on a single line"
        },
        {
          "id": 5,
          "command": "git log --graph --oneline --decorate --all",
          "note": "A simple one line to the point answer"
        },
        {
          "id": 6,
          "command": "git log --stat",
          "note": "See some abbreviated statistics for each commit."
        },
        {
          "id": 7,
          "command": "git log --help",
          "note": "List help options"
        },
        {
          "id": 8,
          "command": "git log -m --name-status",
          "note": "Lists the name and status of each file included in only merge commits."
        }
      ]
    },
    {
      "title": "Starting files",
      "url":
        "https://git-scm.com/book/en/v2/Getting-Started-First-Time-Git-Setup",
      "command": [
        {
          "id": 1,
          "command": "git add file-name.txt",
          "note": "Add a specific file."
        },
        {
          "id": 2,
          "command": "git add file-name.ts another-file.js",
          "note": "Add two different files."
        },
        {
          "id": 3,
          "command": "git add .",
          "note": "Add files located in the root directory."
        },
        {
          "id": 4,
          "command": "git add --all",
          "note":
            "Add files from those other directories plus the files in the root directory to the staging area."
        },
        {
          "id": 5,
          "command": "git rm --cached file-name.txt",
          "note": "Removing files from the staging area."
        },
        {
          "id": 6,
          "command": "git reset file-name.txt",
          "note": "Untrack files."
        },
        {
          "id": 7,
          "command": "git add -i",
          "note": "Add/remove/patch/diff files interactively to and from the staging area."
        }
      ]
    },
    {
      "title": "Commiting",
      "url":
        "https://git-scm.com/book/en/v2/Git-Basics-Recording-Changes-to-the-Repository#_committing_changes",
      "command": [
        {
          "id": 1,
          "command": "git commit -m \"Add three files\"",
          "note":
            "Commit changes to head (but not yet to the remote repository)"
        },
        {
          "id": 2,
          "command": "git commit -a -m \"Do something once more\"",
          "note":
            "Commit any files you've added with git add, and also commit any files you've changed since then."
        },
        {
          "id": 3,
          "command": "git commit --amend -m \"enter your message\"",
          "note": "Make the current changes part of the previous commit."
        },
        {
          "id": 4,
          "command": "git reset --soft HEAD^",
          "note":
            "To restore everything back to the way it was prior to the last commit and if you want to keep your changes."
        },
        {
          "id": 5,
          "command": "git reset --hard origin/master",
          "note":
            "To restore everything back to the way it was prior to the last commit and if you DON'T care about keeping the changes."
        }
      ]
    },
    {
      "title": "Remote repositories",
      "url": "https://git-scm.com/book/en/v2/Git-Basics-Working-with-Remotes",
      "command": [
        {
          "id": 1,
          "command": "git remote add origin https://url",
          "note": "Add a remote origin."
        },
        {
          "id": 2,
          "command": "git remote set-url origin https://url",
          "note": "Set a remote origin"
        },
        {
          "id": 3,
          "command": "git push -u origin master",
          "note": "Copying your code to a remote repository"
        },
        {
          "id": 4,
          "command": "git push -f origin master",
          "note": "Forced update."
        },
        {
          "id": 5,
          "command": "git remote -v",
          "note": "List all remote repositories."
        },
        {
          "id": 6,
          "command": "git clone https://url",
          "note": "Clone an entire project from a remote repository."
        },
        {
          "id": 7,
          "command": "git pull",
          "note": "Update your local repository."
        },
        {
          "id": 8,
          "command": "git tag 1.0.0 commit_id",
          "note": ""
        },
        {
          "id": 9,
          "command": "git tag -a v1.0.0 -m \"my version 1.0.0\"",
          "note": ""
        },
        {
          "id": 10,
          "command": "git show v1.0",
          "note": ""
        }
      ]
    },
    {
      "title": "Branching",
      "url":
        "https://git-scm.com/book/en/v2/Git-Branching-Basic-Branching-and-Merging",
      "command": [
        {
          "id": 1,
          "command": "git branch",
          "note": "See current branche."
        },
        {
          "id": 2,
          "command": "git branch branch-name",
          "note": "Create a new branch."
        },
        {
          "id": 3,
          "command": "git checkout branch-name",
          "note": "Switch branches."
        },
        {
          "id": 4,
          "command": "git checkout -b branch-name",
          "note": "Create a new branch and switch to that branch right away."
        },
        {
          "id": 5,
          "command": "git merge branch-name",
          "note": "Merge Branch."
        }
      ]
    }
  ]
}
